<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="WestendTheatre.GUI.AdminLoginController">
   <children>
      <TextField fx:id="UserName" layoutX="255.0" layoutY="292.0" prefHeight="50.0" prefWidth="200.0" promptText="UserName" />
      <Button layoutX="255.0" layoutY="503.0" mnemonicParsing="false" onAction="#Login" prefHeight="50.0" prefWidth="100.0" text="Sign In" />
      <Label fx:id="Status" layoutX="152.0" layoutY="125.0" prefHeight="60.0" prefWidth="400.0" textAlignment="CENTER" textFill="#da1515" wrapText="true">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <PasswordField fx:id="PassWord" layoutX="255.0" layoutY="371.0" prefHeight="50.0" prefWidth="200.0" promptText="PassWord" />
      <Button layoutX="371.0" layoutY="503.0" mnemonicParsing="false" onAction="#CancelLogin" prefHeight="50.0" prefWidth="100.0" text="Cancel" />
      <Label fx:id="isConnected" layoutX="255.0" layoutY="235.0" prefHeight="50.0" prefWidth="200.0">
         <font>
            <Font name="Arial Bold" size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
