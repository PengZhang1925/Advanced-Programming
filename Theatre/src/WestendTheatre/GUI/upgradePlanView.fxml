<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="WestendTheatre.GUI.upgradePlanController">
   <children>
      <Label layoutX="186.0" layoutY="97.0" text="Upgrade Here!!">
         <font>
            <Font name="Arial Bold" size="48.0" />
         </font>
      </Label>
      <TextField fx:id="memberID" layoutX="211.0" layoutY="300.0" prefHeight="50.0" prefWidth="200.0" promptText="MemberID" />
      <TextField fx:id="planType" layoutX="214.0" layoutY="451.0" prefHeight="50.0" prefWidth="200.0" promptText="PlanType" />
      <Label layoutX="219.0" layoutY="258.0" text="MemberID:">
         <font>
            <Font name="Arial Bold" size="14.0" />
         </font>
      </Label>
      <Label layoutX="220.0" layoutY="416.0" text="Plan:">
         <font>
            <Font name="Arial Bold" size="14.0" />
         </font>
      </Label>
      <Label fx:id="planCheck" layoutX="211.0" layoutY="357.0" prefHeight="38.0" prefWidth="201.0" />
      <Button layoutX="491.0" layoutY="296.0" mnemonicParsing="false" onAction="#checkPlan" prefHeight="50.0" prefWidth="100.0" text="check" />
      <Button layoutX="491.0" layoutY="453.0" mnemonicParsing="false" onAction="#upgradePlan" prefHeight="50.0" prefWidth="100.0" text="Upgrade" />
      <Button layoutX="274.0" layoutY="574.0" mnemonicParsing="false" onAction="#cancelUpgrade" prefHeight="50.0" prefWidth="100.0" text="Back" />
      <Button layoutX="423.0" layoutY="575.0" mnemonicParsing="false" onAction="#jumpToTopUp" prefHeight="50.0" prefWidth="100.0" text="TopUp" />
   </children>
</AnchorPane>
